{
  "name": "minlexbot",
  "version": "1.0.0",
  "description": "App for MinlexBot in telegram",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "bot": "node bot.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "nodejs",
    "node",
    "telegram",
    "bot"
  ],
  "author": "Alexander Istomin <minlex.ais@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.3",
    "express": "^4.17.1",
    "mongoose": "^5.11.16",
    "node-telegram-bot-api": "^0.51.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7"
  }
}
